{
  "expo": {
    "name": "CribUp",
    "slug": "cribup",
    "owner": "arturoh",
    "version": "1.0.0",
    "scheme": "cribup",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#020617"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.arturoh.cribup",
      "usesAppleSignIn": true,
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#020617"
      },
      "package": "com.arturoh.cribup",
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-apple-authentication",
      [
        "expo-notifications",
        {
          "color": "#6366F1"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "ef7b1a66-8ad1-4ad7-9090-6293d86b3b81"
      }
    }
  }
}
